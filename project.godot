config_version=4

_global_script_classes=[ {
"base": "KinematicBody",
"class": "Enemy",
"language": "GDScript",
"path": "res://enemies/Enemy.gd"
} ]
_global_script_class_icons={
"Enemy": ""
}

[application]

config/name="Super Tsunami Surfer"
config/description="Submission for 2024 Pixel Game Jam.
A vertical scrolling endless shooter about an alien surfer dropping meteors into Earth's oceans to ride the gnarliest waves."
run/main_scene="res://interface/Intro.tscn"
config/icon="res://icon.png"

[autoload]

PlayerStats="*res://player/PlayerStats.gd"
Global="*res://Global.tscn"

[display]

window/size/width=256
window/size/height=224
window/size/resizable=false
window/vsync/use_vsync=false
window/stretch/mode="2d"
